{
    "name": "Account Dodger",
    "version": "0.0.0.2",
    "manifest_version": 3,
    "description": "A Chromium extension for reading Quora without logging into an account",
    "icons": {
        "16": "icons/Account_Dodger Logo_1-16.png",
        "32": "icons/Account_Dodger Logo_1.png",
        "48": "icons/Account_Dodger Logo_1-48.png",
        "128": "icons/Account_Dodger Logo_1-128.png"
    },
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "main_code.js"
      },
    "permissions": [
        "tabs"
    ]
}
